		"input.foreground": "#CCCCCC",
		"input.placeholderForeground": "#989898",
		"inputOption.activeBackground": "#ff660033",
		"inputOption.activeBorder": "#ff6600",
		"keybindingLabel.foreground": "#CCCCCC",
		"list.activeSelectionBackground": "#ff660033",
		"list.activeSelectionForeground": "#FFFFFF",
		"list.focusBackground": "#ff660022",
		"list.focusForeground": "#CCCCCC",
		"list.hoverBackground": "#ff660011",
		"list.hoverForeground": "#CCCCCC",
		"list.inactiveSelectionBackground": "#ff660022",
		"list.inactiveSelectionForeground": "#CCCCCC",
		"menu.background": "#1F1F1F",
		"menu.selectionBackground": "#ff6600",
		"menu.selectionForeground": "#FFFFFF",
		"menubar.selectionBackground": "#ff660033",
		"menubar.selectionForeground": "#FFFFFF",
		"notificationCenterHeader.background": "#1F1F1F",
		"notificationCenterHeader.foreground": "#CCCCCC",
		"notifications.background": "#1F1F1F",
		"notifications.border": "#2B2B2B",
		"notifications.foreground": "#CCCCCC",
		"panel.background": "#181818",
		"panel.border": "#2B2B2B",
		"panelInput.border": "#2B2B2B",
		"panelTitle.activeBorder": "#ff6600",
		"panelTitle.activeForeground": "#CCCCCC",
		"panelTitle.inactiveForeground": "#9D9D9D",
		"peekViewEditor.background": "#1F1F1F",
		"peekViewEditor.matchHighlightBackground": "#ff660033",
		"peekViewResult.background": "#1F1F1F",
		"peekViewResult.matchHighlightBackground": "#ff660033",
		"pickerGroup.border": "#3C3C3C",
		"progressBar.background": "#ff6600",
		"quickInput.background": "#222222",
		"quickInput.foreground": "#CCCCCC",
		"scrollbarSlider.activeBackground": "#ff660088",
		"scrollbarSlider.background": "#ff660044",
		"scrollbarSlider.hoverBackground": "#ff660066",
		"settings.dropdownBackground": "#313131",
		"settings.dropdownBorder": "#3C3C3C",
		"settings.headerForeground": "#FFFFFF",
		"settings.modifiedItemIndicator": "#ff6600",
		"sideBar.background": "#181818",
		"sideBar.border": "#2B2B2B",
		"sideBar.foreground": "#CCCCCC",
		"sideBarSectionHeader.background": "#181818",
		"sideBarSectionHeader.border": "#2B2B2B",
		"sideBarSectionHeader.foreground": "#CCCCCC",
		"sideBarTitle.foreground": "#CCCCCC",
		"statusBar.background": "#181818",
		"statusBar.border": "#2B2B2B",
		"statusBar.debuggingBackground": "#ff6600",
		"statusBar.debuggingForeground": "#FFFFFF",
		"statusBar.focusBorder": "#ff6600",
		"statusBar.foreground": "#CCCCCC",
		"statusBar.noFolderBackground": "#1F1F1F",
		"statusBarItem.focusBorder": "#ff6600",
		"statusBarItem.prominentBackground": "#ff660033",
		"statusBarItem.remoteBackground": "#ff6600",
		"statusBarItem.remoteForeground": "#FFFFFF",
		"tab.activeBackground": "#1F1F1F",
		"tab.activeBorder": "#ff6600",
		"tab.activeBorderTop": "#ff6600",
		"tab.activeForeground": "#FFFFFF",
		"tab.border": "#2B2B2B",
		"tab.hoverBackground": "#1F1F1F",
		"tab.inactiveBackground": "#181818",
		"tab.inactiveForeground": "#9D9D9D",
		"tab.unfocusedActiveBorder": "#ff660088",
		"tab.unfocusedActiveBorderTop": "#ff660088",
		"terminal.ansiBlack": "#1F1F1F",
		"terminal.ansiBlue": "#4FC1FF",
		"terminal.ansiBrightBlack": "#6E7681",
		"terminal.ansiBrightBlue": "#91CBFF",
		"terminal.ansiBrightCyan": "#76E3EA",
		"terminal.ansiBrightGreen": "#4AE168",
		"terminal.ansiBrightMagenta": "#D29BFE",
		"terminal.ansiBrightRed": "#FF6B6B",
		"terminal.ansiBrightWhite": "#FFFFFF",
		"terminal.ansiBrightYellow": "#FFD866",
		"terminal.ansiCyan": "#39C5CF",
		"terminal.ansiGreen": "#2EA043",
		"terminal.ansiMagenta": "#BC8CFF",
		"terminal.ansiRed": "#F85149",
		"terminal.ansiWhite": "#CCCCCC",
		"terminal.ansiYellow": "#FFA657",
		"textBlockQuote.background": "#2B2B2B",
		"textBlockQuote.border": "#ff6600",
		"textCodeBlock.background": "#2B2B2B",
		"textLink.activeForeground": "#ff7722",
		"textLink.foreground": "#ff6600",
		"textPreformat.foreground": "#CCCCCC",
		"titleBar.activeBackground": "#181818",
		"titleBar.activeForeground": "#CCCCCC",
		"titleBar.border": "#2B2B2B",
		"titleBar.inactiveBackground": "#181818",
		"titleBar.inactiveForeground": "#9D9D9D",
		"tree.indentGuidesStroke": "#3C3C3C",
		"welcomePage.buttonBackground": "#313131",
		"welcomePage.buttonHoverBackground": "#3C3C3C"
	},
	"tokenColors": [
		{
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#6E7681"
			}
		},
		{
			"scope": [
				"constant",
				"entity.name.constant",
				"variable.other.constant",
				"variable.language"
			],
			"settings": {
				"foreground": "#79C0FF"
			}
		},
		{
			"scope": [
				"entity",
				"entity.name"
			],
			"settings": {
				"foreground": "#D2A8FF"
			}
		},
		{
			"scope": "variable.parameter.function",
			"settings": {
				"foreground": "#CCCCCC"
			}
		},
		{
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#7EE787"
			}
		},
		{
			"scope": "keyword",
			"settings": {
				"foreground": "#FF7B72"
			}
		},
		{
			"scope": [
				"storage",
				"storage.type"
			],
			"settings": {
				"foreground": "#FF7B72"
			}
		},
		{
			"scope": [
				"storage.modifier.package",
				"storage.modifier.import",
				"storage.type.java"
			],
			"settings": {
				"foreground": "#CCCCCC"
			}
		},
		{
			"scope": [
				"string",
				"punctuation.definition.string",
				"string punctuation.section.embedded source"
			],
			"settings": {
				"foreground": "#A5D6FF"
			}
		},
		{
			"scope": "support",
			"settings": {
				"foreground": "#79C0FF"
			}
		},
		{
			"scope": "meta.property-name",
			"settings": {
				"foreground": "#79C0FF"
			}
		},
		{
			"scope": "variable",
			"settings": {
				"foreground": "#FFA657"
			}
		},
		{
			"scope": "variable.other",
			"settings": {
				"foreground": "#CCCCCC"
			}
		},
		{
			"scope": "invalid.broken",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#FFA198"
			}
		},
		{
			"scope": "invalid.deprecated",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#FFA198"
			}
		},
		{
			"scope": "invalid.illegal",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#FFA198"
			}
		},
		{
			"scope": "invalid.unimplemented",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#FFA198"
			}
		},
		{
			"scope": "carriage-return",
			"settings": {
				"fontStyle": "italic underline",
				"foreground": "#F0F6FC",
				"background": "#B62324"
			}
		},
		{
			"scope": "message.error",
			"settings": {
				"foreground": "#FFA198"
			}
		},
		{
			"scope": "string source",
			"settings": {
				"foreground": "#CCCCCC"
			}
		},
		{
			"scope": "string variable",
			"settings": {
				"foreground": "#79C0FF"
			}
		},
		{
			"scope": [
				"source.regexp",
				"string.regexp"
			],
			"settings": {
				"foreground": "#A5D6FF"
			}
		},
		{
			"scope": [
				"string.regexp.character-class",
				"string.regexp constant.character.escape",
				"string.regexp source.ruby.embedded",
				"string.regexp string.regexp.arbitrary-repitition"
			],
			"settings": {
				"foreground": "#A5D6FF"
			}
		},
		{
			"scope": "string.regexp constant.character.escape",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#7EE787"
			}
		},
		{
			"scope": "support.constant",
			"settings": {
				"foreground": "#79C0FF"
			}
		},
		{
			"scope": "support.variable",
			"settings": {
				"foreground": "#79C0FF"
			}
		},
		{
			"scope": "meta.module-reference",
			"settings": {
				"foreground": "#79C0FF"
			}
		},
		{
			"scope": "punctuation.definition.list.begin.markdown",
			"settings": {
				"foreground": "#FFA657"
			}
		},
		{
			"scope": [
				"markup.heading",
				"markup.heading entity.name"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#79C0FF"
			}
		},
		{
			"scope": "markup.quote",
			"settings": {
				"foreground": "#7EE787"
			}
		},
		{
			"scope": "markup.italic",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#CCCCCC"
			}
		},
		{
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#CCCCCC"
			}
		},
		{
			"scope": "markup.raw",
			"settings": {
				"foreground": "#79C0FF"
			}
		},
		{
			"scope": [
				"markup.deleted",
				"meta.diff.header.from-file",
				"punctuation.definition.deleted"
			],
			"settings": {
				"background": "#490202",
				"foreground": "#FFA198"
			}
		},
		{
			"scope": [
				"markup.inserted",
				"meta.diff.header.to-file",
				"punctuation.definition.inserted"
			],
			"settings": {
				"background": "#04260F",
				"foreground": "#7EE787"
			}
		},
		{
			"scope": [
				"markup.changed",
				"punctuation.definition.changed"
			],
			"settings": {
				"background": "#5A1E02",
				"foreground": "#FFA657"
			}
		},
		{
			"scope": [
				"markup.ignored",
				"markup.untracked"
			],
			"settings": {
				"foreground": "#161B22",
				"background": "#79C0FF"
			}
		},
		{
			"scope": "meta.diff.range",
			"settings": {
				"foreground": "#D2A8FF",
				"fontStyle": "bold"
			}
		},
		{
			"scope": "meta.diff.header",
			"settings": {
				"foreground": "#79C0FF"
			}
		},
		{
			"scope": "meta.separator",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#79C0FF"
			}
		},
		{
			"scope": "meta.output",
			"settings": {
				"foreground": "#79C0FF"
			}
		},
		{
			"scope": [
				"brackethighlighter.tag",
				"brackethighlighter.curly",
				"brackethighlighter.round",
				"brackethighlighter.square",
				"brackethighlighter.angle",
				"brackethighlighter.quote"
			],
			"settings": {
				"foreground": "#6E7681"
			}
		},
		{
			"scope": "brackethighlighter.unmatched",
			"settings": {
				"foreground": "#FFA198"
			}
		},
		{
			"scope": [
				"constant.other.reference.link",
				"string.other.link"
			],
			"settings": {
				"foreground": "#A5D6FF",
				"fontStyle": "underline"
			}
		}
	]
}
